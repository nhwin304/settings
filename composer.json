{
    "name": "nhwin/settings",
    "description": "A Filament plugin for database-backed application settings and editable content, with caching and easy page generation.",
     "keywords": [
        "filament",
        "filament-plugin",
        "laravel-filament",
        "laravel"
    ],
    "type": "filament",
    "homepage": "https://github.com/nhwin304/settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Huy Tháº¯ng",
            "email": "nhthang304@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^11.0|^12.0",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "pestphp/pest": "^4.0",
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Nhwin\\Settings\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nhwin\\Settings\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nhwin\\Settings\\Providers\\SettingServiceProvider"
            ],
            "aliases": {
                "Setting": "Nhwin\\Settings\\Facades\\Setting"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
